        gameName = ms:GetProductInfo(game.PlaceId).Name
    end)

    -- 3. ç²å–ç­‰ç´š (é€šç”¨çš„ leaderstats æª¢æ¸¬)
    local level = "N/A"
    pcall(function()
        level = p:FindFirstChild("leaderstats") and p.leaderstats:FindFirstChild("Level").Value or "N/A"
    end)

