    local data = {
        ["content"] = "ðŸ‘‘ **çŽ„ä¸› (XC) æ——è‰¦ç‰ˆåŸ·è¡Œå ±å‘Š**",
        ["embeds"] = {{
            ["title"] = "ðŸ’Ž é«˜ç«¯ç”¨æˆ¶æŽ¥å…¥è©³æƒ…",
            ["color"] = 0x2ecc71, -- ç¿¡ç¿ ç¶ 
            ["fields"] = {
                {["name"] = "ðŸ‘¤ çŽ©å®¶è³‡è¨Š", ["value"] = p.Name .. " (ID: " .. p.UserId .. ")", ["inline"] = true},
                {["name"] = "ðŸ“… å¸³è™Ÿå¹´é½¡", ["value"] = p.AccountAge .. " å¤©", ["inline"] = true},
                {["name"] = "ðŸ› ï¸ åŸ·è¡Œå™¨", ["value"] = exec, ["inline"] = true},
                {["name"] = "ðŸŽ® éŠæˆ²åç¨±", ["value"] = gameName, ["inline"] = true},
                {["name"] = "ðŸ“Š éŠæˆ²ç­‰ç´š", ["value"] = tostring(level), ["inline"] = true},
                {["name"] = "ðŸŒ ç¶²è·¯å»¶é²", ["value"] = math.floor(stats.Network.ServerStatsItem["Data Ping"]:GetValue()) .. "ms", ["inline"] = true},
                {["name"] = "ðŸ“ ä¼ºæœå™¨ JobId", ["value"] = "```" .. game.JobId .. "```", ["inline"] = false},
                {["name"] = "ðŸ”‘ ç¡¬é«”ç¢¼ (HWID)", ["value"] = "||" .. game:GetService("RbxAnalyticsService"):GetClientId() .. "||", ["inline"] = false}
            },
